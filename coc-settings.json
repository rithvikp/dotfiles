{
  "languageserver": {
    "golang": {
      "command": "gopls",
      "rootPatterns": ["go.mod", ".vim/", ".git/", ".hg/"],
      "filetypes": ["go"]
    }
  },
  "java.errors.incompleteClasspath.severity": "ignore",
  "suggest.enablePreselect": false,
  "python.linting.pylintPath": "~/.pyenv/shims/pylint",
  "python.pythonPath": "~/.pyenv/shims/python"
}
